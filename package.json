{
  "name": "pdfjs-vue",
  "private": true,
  "version": "0.0.0",
  "description": "Using pdfjs to display PDFs as a vue 3 component.",
  "files": [
    "dist/pdfjs*",
    "dist/style*"
  ],
  "main": "./dist/pdfjs-vue.umd.js",
  "module": "./dist/pdfjs-vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/pdfjs-vue.es.js",
      "require": "./dist/pdfjs-vue.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "pdfjs-dist": "^2.13.216",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.2.4",
    "typescript": "^4.6.3",
    "vite": "^2.8.6",
    "vue-tsc": "^0.33.9"
  }
}
